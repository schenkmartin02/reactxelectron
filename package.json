    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "electron": "^18.2.3",
    "electron-is-dev": "^2.0.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "wait-on": "^6.0.1"
